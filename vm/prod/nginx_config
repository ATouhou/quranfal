
upstream django {
    server unix:/tmp/quranfalweb/uwsgi.sock; # for a file socket
}

server {
    listen 80;
    server_name 0.0.0.0;

    location = /favicon.ico {
        access_log off;
        log_not_found off;
    }

    location /static/ {
        alias /home/quran/quranfalweb/static_collected/;
        #root /home/quran/quranfalweb;
    }

    location / {
        uwsgi_pass  django;
        include     /home/quran/quranfalweb/vms/aws/uwsgi_params;
    }

}